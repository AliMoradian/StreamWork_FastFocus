<?xml version="1.0" encoding="UTF-8" ?>
<Module> 
  <ModulePrefs title="DealersChoice">
    <Require feature="osapi" /> 
  </ModulePrefs> 
  <Content type="html">
    <![CDATA[


<div class="dRetrievedInfo" > </div>

<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js"></script>
<script src="http://code.jquery.com/ui/1.8.18/jquery-ui.min.js" type="text/javascript"></script>
<script>
var str = "";
osapi.people.getViewer({fields: ['name', 'birthday']}).execute(function(result) {
  str += "Your display name is " + result.displayName;
  str += "<br />Your id is " + result.id ;
});


$(".dRetrievedInfo").html(str);
    </script>
   ]]>
